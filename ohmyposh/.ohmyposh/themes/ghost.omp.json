{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "arrow_1": "#b6fe42",
    "arrow_2": "#ff7f81",
    "arrow_3": "#88ecf9",
    "bg_base": "#eca5a5",
    "bg_filepath": "#b7cdd0",
    "bg_modified": "#fddf9f",
    "bg_stable": "#a3f9b8",
    "bg_unstable": "#e67e80",
    "cyan": "#7fbbb3",
    "text": "#3B4252"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "arch": "\uf303",
            "linux": "\ue712",
            "ubuntu": "\uf31c"
          },
          "template": " {{ .Icon }} ",
          "foreground": "p:text",
          "background": "p:bg_base",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf07b ",
            "folder_separator_icon": "/",
            "home_icon": "\uf015 ",
            "max_depth": 3,
            "style": "full"
          },
          "template": " <p:text>\uf07b</> {{ .Path }} ",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b0",
          "background": "p:bg_filepath",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_behind_icon": "↓",
            "branch_gone_icon": "\uf00d",
            "branch_icon": "\uf126 ",
            "branch_identical_icon": "≡",
            "branch_max_length": 25,
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "trailing_diamond": "\ue0b1",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <transparent>\ue0b1</> <p:text>\uf040 {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }} <transparent>\ue0b1</> <p:text>\uf067 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <transparent>\ue0b1</> <p:text>\uf187 {{ .StashCount }}</>{{ end }} ",
          "foreground": "p:text",
          "background": "p:bg_stable",
          "type": "git",
          "style": "diamond",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:bg_modified{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:bg_unstable{{ end }}",
            "{{ if gt .Ahead 0 }}p:bg_stable{{ end }}",
            "{{ if gt .Behind 0 }}p:bg_stable{{ end }}"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "austin"
          },
          "template": "<p:text></> \uf469 ",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b2",
          "background": "p:bg_base",
          "type": "exit",
          "style": "powerline",
          "background_templates": ["{{ if gt .Code 0 }}p:bg_unstable{{ end }}"],
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "p:text",
          "background": "p:green",
          "type": "node",
          "style": "diamond",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " \ue71e {{ .Full }} ",
          "foreground": "p:text",
          "background": "p:green",
          "type": "npm",
          "style": "diamond",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " \ufcd1 {{ .Full }} ",
          "foreground": "p:text",
          "background": "p:green",
          "type": "go",
          "style": "diamond",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " \ue738 {{ .Full }}",
          "foreground": "p:text",
          "background": "p:green",
          "type": "java",
          "style": "diamond",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " \ue235 {{ .Full }} ",
          "foreground": "p:text",
          "background": "p:green",
          "type": "python",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2570\u2500 <b>{{ .UserName }}</b> <p:arrow_1>❯</><p:arrow_2>❯</><p:arrow_3>❯</>",
          "foreground": "p:bg_base",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
